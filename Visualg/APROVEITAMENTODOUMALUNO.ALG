algoritmo "AproveitamentodoUmAluno"
var
       n1, n2 ,media ,nota :Real
inicio
       Escreva ("Primeira Nota" )
       leia (n1)
       Escreva ("Segunda Nota" )
       Leia (n2)
       media   <- (n1+n2)/2
       se
         media <- >= 5
         
         
         
       Escreval ("Media :", media)
       Escreval ("Aproveitamento: " )
       
       
fimalgoritmo