Algoritmo "Creuza01"

Var

       anoAtual, anoNascimento, resposta: Inteiro

Inicio

        Escreva ("Em que ano nós estamos?")
        Leia (anoAtual)
        Escreva ("Em que ano eu nasci?")
        Leia (anoNascimento)
        resposta <- anoAtual - anoNascimento
        Escreva ("Eu tenho ",resposta, " anos")

        
Fimalgoritmo